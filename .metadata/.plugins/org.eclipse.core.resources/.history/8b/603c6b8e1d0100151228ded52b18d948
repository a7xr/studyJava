package StudyJava;

public class StuffsTest {

}

class ThreadTest01 extends Thread{
	
	String str = "";
	int wait = 500, times = 5;
	
	@Override
	public void run() {
		str = "this is a test";
		for (int i = 0; i < times; i++) {
			System.out.println(this.str);
		}
	}
	
	public void soping(String txt) {
		System.out.println(txt);
	}	
}